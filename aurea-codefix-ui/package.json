{
  "name": "codefix",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "mockServer": "nodemon --watch json-server/data --watch json-server --exec 'ts-node' json-server/server.ts",
    "mockServer:startMock": "concurrently --kill-others \"npm run mockServer\" \"npm run startMock\"",
    "startLocal": "ng serve --proxy-config proxy/proxy.conf.json",
    "startDev": "ng serve --proxy-config proxy/proxy-dev.conf.json",
    "startQa": "ng serve --proxy-config proxy/proxy-qa.conf.json",
    "startStaging": "ng serve --proxy-config proxy/proxy-staging.conf.json",
    "startMock": "ng serve --proxy-config proxy/proxy.mock-conf.json",
    "buildProd": "ng build --aot --configuration=production",
    "build": "ng build",
    "test": "ng test --watch=false --code-coverage",
    "testDebug": "ng test --watch=true --browsers Chrome",
    "lint": "ng lint --force --format pmd > ./build/reports/pmd/ui_pmd_report.xml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@devfactory/ngx-df": "6.13.0",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@types/card-validator": "^4.1.0",
    "@types/dagre-d3": "^0.4.38",
    "auth0-js": "^9.10.1",
    "card-validator": "^6.1.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "intl": "^1.2.5",
    "mdn-polyfills": "^5.13.0",
    "rxjs": "~6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^6.0.2",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@devfactory/schematics": "^1.5.0",
    "@devfactory/tslint-devfactory-rules": "^1.2.0",
    "@types/auth0-js": "^8.11.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.2",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "nodemon": "^1.18.11",
    "protractor": "~5.4.0",
    "ts-node": "^8.1.0",
    "tslint": "~5.11.0",
    "typescript": "^2.9.0"
  }
}
